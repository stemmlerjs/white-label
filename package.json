{
  "name": "white-label",
  "version": "1.0.0",
  "description": "A Vinyl-Trading enterprise app built with Node.js + TypeScript using Domain-Driven Design",
  "main": "bin/www",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.21.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "sequelize": "^5.8.12",
    "shallow-equal-object": "^1.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/cls-hooked": "^4.3.0",
    "@types/jest": "^24.0.15",
    "cross-env": "^5.2.0",
    "dotenv-cli": "^2.0.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "sequelize-cli": "^5.5.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "db:create:dev": "cross-env NODE_ENV=development node scripts/db/create",
    "db:create:test": "cross-env NODE_ENV=test node scripts/db/create",
    "db:delete:dev": "cross-env NODE_ENV=development node scripts/db/delete",
    "db:delete:test": "cross-env NODE_ENV=test node scripts/db/delete",
    "db:flush:dev": "cross-env NODE_ENV=development npm run db:delete:dev && npm run db:create:dev",
    "db:flush:test": "cross-env NODE_ENV=test npm run db:delete:test && npm run db:create:test",
    "migrate:dev": "npm run build && cross-env NODE_ENV=development dotenv -e .env sequelize db:migrate",
    "migrate:test": "npm run build && cross-env NODE_ENV=test dotenv -e .env sequelize db:migrate --env test",
    "migrate:prod": "npm run build && cross-env NODE_ENV=production dotenv -e .env sequelize db:migrate --env production",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "testWithCoverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stemmlerjs/white-label.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "domain-driven-design"
  ],
  "author": "Khalil Stemmler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stemmlerjs/white-label/issues"
  },
  "homepage": "https://github.com/stemmlerjs/white-label#readme"
}
